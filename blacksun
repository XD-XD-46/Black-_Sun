from turtle import *

t1 = Turtle()
t2 = Turtle()
t3 = Turtle()
t4 = Turtle()
t5 = Turtle()
t6 = Turtle()
t7 = Turtle()
t8 = Turtle()
t9 = Turtle()
t10 = Turtle()
t11 = Turtle()
t12 = Turtle()
t13 = Turtle()
t14 = Turtle()

t13.goto(0,-205)
t13.speed(1000)
t13.width(15)
for i in range(1):
    t13.circle(210)

t14.speed(1000)
t14.goto(0,-55)
t14.width(15)
for i in range(120):
    t14.fd(3)
    t14.lt(3)

t1.speed(1000)
t1.goto(0,0)
t1.width(15)
for i in range(1):
    t1.lt(90)
    t1.fd(150)
    t1.rt(90)
    t1.fd(50)
    t1.lt(90)
    t1.fd(50)


